<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminViewUsers">

    <TextView
        android:id="@+id/textViewUsers"
        android:layout_width="179dp"
        android:layout_height="68dp"
        android:gravity="center"
        android:text="@string/users"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/textViewUsersList"
        android:layout_width="348dp"
        android:layout_height="328dp"
        android:gravity="center"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="133dp" />

    <Button
        android:id="@+id/buttonMainScreenFromUsers"
        android:layout_width="177dp"
        android:layout_height="108dp"
        android:text="@string/return_to_main_screen"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="544dp" />
</androidx.constraintlayout.widget.ConstraintLayout>